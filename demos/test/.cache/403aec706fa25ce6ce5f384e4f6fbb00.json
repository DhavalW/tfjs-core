arningRate(learningRate: number) {\n    this.learningRate = learningRate;\n    if (this.c != null) {\n      this.c.dispose();\n    }\n    this.c = keep(scalar(-learningRate));\n  }\n\n  dispose() {\n    this.c.dispose();\n  }\n}\n"},"lineCount":null}},"hash":"8c624534f016bb1de9ddcc9de73f656d","cacheData":{"env":{}}}